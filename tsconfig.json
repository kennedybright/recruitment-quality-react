{
  "compilerOptions": {
    "noImplicitAny": false,
    "target": "ES2017",
    "lib": ["es2017", "es6", "dom", "dom.iterable", "esnext"],
    "declaration": true,
    "declarationMap": true,
    "declarationDir": "../panel-recruitment-quality-frontend/build/usremoterecqa/types",
    "allowJs": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "ESNext",
    "baseUrl": ".",
    "experimentalDecorators": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "outDir": "../panel-recruitment-quality-frontend/build",
    "pretty": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "preserveSymlinks": true
  },
  "exclude": ["node_modules"],
  "include": [ "./src/**/*"],
}
